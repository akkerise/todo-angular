#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/var/www/html/todo-angular/node_modules/node-sass/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules" "$basedir/node"  "$basedir/../node-sass/bin/node-sass" "$@"
  ret=$?
else 
  NODE_PATH="/var/www/html/todo-angular/node_modules/node-sass/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules" node  "$basedir/../node-sass/bin/node-sass" "$@"
  ret=$?
fi
exit $ret
