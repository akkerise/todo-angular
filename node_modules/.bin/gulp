#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/gulp/3.9.1/node_modules/gulp/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/gulp/3.9.1/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/gulp/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/gulp/node_modules" "$basedir/node"  "$basedir/../gulp/bin/gulp.js" "$@"
  ret=$?
else 
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/gulp/3.9.1/node_modules/gulp/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/gulp/3.9.1/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/gulp/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/gulp/node_modules" node  "$basedir/../gulp/bin/gulp.js" "$@"
  ret=$?
fi
exit $ret
