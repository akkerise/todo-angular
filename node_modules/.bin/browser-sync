#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync/2.18.12/node_modules/browser-sync/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync/2.18.12/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/browser-sync/node_modules" "$basedir/node"  "$basedir/../browser-sync/bin/browser-sync.js" "$@"
  ret=$?
else 
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync/2.18.12/node_modules/browser-sync/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync/2.18.12/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/browser-sync/node_modules" node  "$basedir/../browser-sync/bin/browser-sync.js" "$@"
  ret=$?
fi
exit $ret
