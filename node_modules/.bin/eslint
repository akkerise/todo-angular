#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/eslint/4.3.0/node_modules/eslint/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/eslint/4.3.0/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/eslint/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/eslint/node_modules" "$basedir/node"  "$basedir/../eslint/bin/eslint.js" "$@"
  ret=$?
else 
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/eslint/4.3.0/node_modules/eslint/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/eslint/4.3.0/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/eslint/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/eslint/node_modules" node  "$basedir/../eslint/bin/eslint.js" "$@"
  ret=$?
fi
exit $ret
