#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/optipng-bin/3.1.4/node_modules/optipng-bin/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/optipng-bin/3.1.4/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/optipng-bin/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/imagemin-optipng/5.2.1/node_modules/optipng-bin/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/imagemin-optipng/5.2.1/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/imagemin-optipng/node_modules" "$basedir/node"  "$basedir/../../../optipng-bin/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/optipng-bin/3.1.4/node_modules/optipng-bin/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/optipng-bin/3.1.4/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/optipng-bin/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/imagemin-optipng/5.2.1/node_modules/optipng-bin/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/imagemin-optipng/5.2.1/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/imagemin-optipng/node_modules" node  "$basedir/../../../optipng-bin/cli.js" "$@"
  ret=$?
fi
exit $ret
