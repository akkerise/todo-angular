#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/fsevents/1.1.2/node_modules/fsevents/node_modules/sshpk/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/fsevents/1.1.2/node_modules/fsevents/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/fsevents/1.1.2/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/fsevents/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules" "$basedir/node"  "$basedir/../sshpk/bin/sshpk-conv" "$@"
  ret=$?
else 
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/fsevents/1.1.2/node_modules/fsevents/node_modules/sshpk/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/fsevents/1.1.2/node_modules/fsevents/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/fsevents/1.1.2/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/fsevents/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules" node  "$basedir/../sshpk/bin/sshpk-conv" "$@"
  ret=$?
fi
exit $ret
