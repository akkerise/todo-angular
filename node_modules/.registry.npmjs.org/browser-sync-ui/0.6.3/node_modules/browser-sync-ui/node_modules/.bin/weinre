#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/weinre/2.0.0-pre-I0Z7U9OV/node_modules/weinre/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/weinre/2.0.0-pre-I0Z7U9OV/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/weinre/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync-ui/0.6.3/node_modules/weinre/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync-ui/0.6.3/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync-ui/node_modules" "$basedir/node"  "$basedir/../../../weinre/weinre" "$@"
  ret=$?
else 
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/weinre/2.0.0-pre-I0Z7U9OV/node_modules/weinre/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/weinre/2.0.0-pre-I0Z7U9OV/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/weinre/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync-ui/0.6.3/node_modules/weinre/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync-ui/0.6.3/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/browser-sync-ui/node_modules" node  "$basedir/../../../weinre/weinre" "$@"
  ret=$?
fi
exit $ret
