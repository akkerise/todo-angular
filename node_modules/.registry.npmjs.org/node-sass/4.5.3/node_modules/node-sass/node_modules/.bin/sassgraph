#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules/sass-graph/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sass-graph/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-sass/4.5.3/node_modules/sass-graph/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-sass/4.5.3/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-sass/node_modules" "$basedir/node"  "$basedir/../../../sass-graph/bin/sassgraph" "$@"
  ret=$?
else 
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules/sass-graph/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sass-graph/2.2.4/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sass-graph/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-sass/4.5.3/node_modules/sass-graph/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-sass/4.5.3/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-sass/node_modules" node  "$basedir/../../../sass-graph/bin/sassgraph" "$@"
  ret=$?
fi
exit $ret
