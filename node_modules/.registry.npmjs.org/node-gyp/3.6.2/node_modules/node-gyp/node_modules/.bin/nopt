#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules/nopt/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/nopt/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-gyp/3.6.2/node_modules/nopt/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-gyp/3.6.2/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-gyp/node_modules" "$basedir/node"  "$basedir/../../../nopt/bin/nopt.js" "$@"
  ret=$?
else 
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules/nopt/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/nopt/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-gyp/3.6.2/node_modules/nopt/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-gyp/3.6.2/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node-gyp/node_modules" node  "$basedir/../../../nopt/bin/nopt.js" "$@"
  ret=$?
fi
exit $ret
