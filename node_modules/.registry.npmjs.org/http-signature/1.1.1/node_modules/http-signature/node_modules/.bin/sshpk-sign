#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sshpk/1.13.1/node_modules/sshpk/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sshpk/1.13.1/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sshpk/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/http-signature/1.1.1/node_modules/sshpk/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/http-signature/1.1.1/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/http-signature/node_modules" "$basedir/node"  "$basedir/../../../sshpk/bin/sshpk-sign" "$@"
  ret=$?
else 
  NODE_PATH="/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sshpk/1.13.1/node_modules/sshpk/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sshpk/1.13.1/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/sshpk/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/node_modules:/var/www/html/todo-angular/node_modules:/var/www/html/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/http-signature/1.1.1/node_modules/sshpk/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/http-signature/1.1.1/node_modules:/var/www/html/todo-angular/node_modules/.registry.npmjs.org/http-signature/node_modules" node  "$basedir/../../../sshpk/bin/sshpk-sign" "$@"
  ret=$?
fi
exit $ret
